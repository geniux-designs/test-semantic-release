{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "release": {
      "dependsOn": ["^release"],
      "cache": false
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}